# Backend Environment Variables

# Server Configuration
PORT=3001
NODE_ENV=development

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# Prokerala API (OAuth access token for real astrology data)
# Option 1: Use access token directly (expires periodically)
PROKERALA_ACCESS_TOKEN=your_prokerala_oauth_access_token_here

# Option 2: Use client credentials for automatic token refresh
PROKERALA_CLIENT_ID=your_prokerala_client_id_here
PROKERALA_CLIENT_SECRET=your_prokerala_client_secret_here

PROKERALA_API_BASE=https://api.prokerala.com/v2/astrology

# Geocoding API (optional - uses free OpenStreetMap by default)
GEOCODING_API_KEY=your_google_maps_api_key_here

# Walrus Storage (Tusky SDK)
WALRUS_API_KEY=your_walrus_api_key_here
WALRUS_API_URL=https://api.walrus.storage

# Self Verification (if needed for backend verification)
SELF_VERIFICATION_SECRET=your_self_verification_secret_here

# Database (if needed in the future)
DATABASE_URL=your_database_url_here

# JWT Secret (if needed for authentication)
JWT_SECRET=your_jwt_secret_here

# File Upload
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./uploads

# Chart Generation
CHART_WIDTH=800
CHART_HEIGHT=600 